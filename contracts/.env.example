# Deployment Configuration
# Copy this file to .env and fill in your values

# Deployer private key (will be the admin/owner of contracts)
# WARNING: Never commit this to git! Use testnet wallet only!
PRIVATE_KEY=your_private_key_here

# Mezo testnet RPC URL
RPC_URL=https://mezo-testnet-rpc-url

# MUSD token address on Mezo testnet
MUSD_ADDRESS=0x0000000000000000000000000000000000000000

# Platform fee wallet address (where 1% fees will be sent)
PLATFORM_FEE_WALLET=0x0000000000000000000000000000000000000000

# Chain ID
CHAIN_ID=12345

# Block explorer API key (optional, for contract verification)
ETHERSCAN_API_KEY=your_api_key_here
